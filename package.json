{
  "name": "JewanApparel",
  "version": "1.0.0",
  "description": "E-Commerce website for Jewan Apparel, built with the MERN stack",
  "type": "module",
  "main": "server2.js",
  "scripts": {
    "start": "node server2/./bin/www",
    "server": "nodemon server2/server2.js",
    "client": " cd client && yarn start",
    "dev": "concurrently \"yarn server\" \"yarn client\"",
    "data:import": "node server/seeder.js",
    "data:destroy": "node server/seeder.js -d"
  },
  "repository": "https://github.com/Acekingcoder/JEWAN-APPAREL.git",
  "author": "Kennedy Ehigiator",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.18",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "5.11.15"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.4",
    "concurrently": "^8.2.1",
    "dotenv": "^16.3.1",
    "nodemon": "^3.0.1"
  }
}
